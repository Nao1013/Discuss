<div class="container">
  <div class="row">
<div class="center-block mx-auto col-6">
<h4>投稿一覧</h4>

<table>
  <tr>
    <th>Title</th>
    <th></th>
  </tr>
 
  <% @posts.each do |post| %>
    <tr>
      <td><%= link_to post.title, post_path(post.id) %></td>
      <td><%= link_to '編集', edit_post_path(post.id) %></td>
      <td><%= link_to '削除', post_path(post.id), method: :delete, "data-confirm" => "本当に削除しますか？" %></td>
    </tr>
  <% end %>
</table>
</div>
</div>
</div>