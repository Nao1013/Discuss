<div class="container">
  <div class="row">
    <div class="col">
      <h3>新規投稿</h3>
      <%= form_with model: @post do |f| %>
    <h4>タイトル</h4>
      <%= f.text_field :title %>
    <h4>内容</h4>
      <%= f.text_area :body %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*", multiple: true %>
    <%= f.submit '投稿' %>
  <% end %>
    </div>
  </div>
</div>